<launch>
      <node pkg="bridge_px4" type="LQR.py" name ="lqr" output="screen">
            <!-- <param name="traj_name" value="2024_05_16_kick_uxyz_gx.npz"/> -->
            <!-- <param name="traj_name" value="2024_05_22_kick_uxyz_gx_dronexlim.npz"/> -->
            <param name="traj_name" value="2024_05_22_kick_uxyz_gx_dronexlim_tunevinelength.npz"/>
            <param name="feedback_multiplier" value="0"/>
      </node>

      <node pkg="bridge_px4" type="koopman_state_publisher.py" name ="koopman_state_publisher" output="screen"/>

      <node pkg="rosbag" type="record" name="rosbag_record_gcs"
            args="/drone5/mavros/vision_pose/pose 
                  /drone5/mavros/local_position/pose
                  /drone5/mavros/setpoint_position/local
                  /drone5/setpoint/position
                  /drone5/setpoint/attitude
                  /vrpn_client_node/tip/pose
                  /drone5/mavros/imu/data
                  /gcs/setpoint/position
                  /gcs/setpoint/attitude
                  /koopman_state"/>
